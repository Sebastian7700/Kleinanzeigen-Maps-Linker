{
    "manifest_version": 3,
    "name": "Kleinanzeigen Maps Linker",
    "version": "1.0",
    "description": "Springen Sie direkt von der Kleinanzeige zum genauen Standort auf Google Maps.",
    "icons": {
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["*://www.kleinanzeigen.de/*"],
        "js": ["content.js"]
      }
    ]
  }